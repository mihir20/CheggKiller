{
    "manifest_version": 2,
    "name": "Chegg Killer",
    "version": "1.0.0",
    "icons": {
        "128": "ic_launcher.png"
    },
    "browser_action": {
        "default_icon": "ic_launcher.png"
    },
    "content_scripts":[{
        "matches": [
            "<all_urls>"
        ],
        "js":["killerScript.js"]
    }
    ],
    "permissions": ["activeTab"]
}